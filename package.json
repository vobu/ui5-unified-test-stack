{
	"name": "single.teststack",
	"version": "1.0.0",
	"description": "UI5 Application: single.teststack",
	"author": "Volker Buzek <volker.buzek@js-soft.com>",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"start": "ui5 serve --port 8080 -o index.html",
		"start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"//test:old": "npm run lint && npm run karma-ci-cov",
		"_start": "ui5 serve --port 8080",
		"_test": "wait-on tcp:8080 & run-p test:*",
		"test": "run-p -r _start _test",
		"test:unit": "wdio run ./webapp/test/unit/wdio.conf.unit.js",
		"test:integration": "wdio run ./webapp/test/integration/wdio.conf.integration.js",
		"test:e2e": "npm run wdi5",
		"wdi5": "wdio run ./webapp/test/e2e/wdio.conf.e2e.js"
	},
	"devDependencies": {
		"@ui5/cli": "^3.6.1",
		"@ui5/middleware-code-coverage": "^1.1.0",
		"@wdio/cli": "^8.19.0",
		"@wdio/local-runner": "^8.19.0",
		"@wdio/mocha-framework": "^8.19.0",
		"@wdio/spec-reporter": "^8.19.0",
		"eslint": "^8.50.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^3.0.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^5.0.5",
		"ui5-middleware-livereload": "^3.0.1",
		"wait-on": "^7.0.1",
		"wdio-qunit-service": "^0.1.0",
		"wdio-ui5-service": "^2.0.2"
	}
}
